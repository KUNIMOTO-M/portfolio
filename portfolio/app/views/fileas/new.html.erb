	<%= form_with model: @file do |f| %>

        <%= f.label :title %>
        <%= f.text_field :title, class: "form-control u_set_li"%>

        <%= f.label :content %>
        <%= f.text_field :content, class: "form-control u_set_li"%>
        
        <%= f.label :file%>
        <%= f.file_field :file, class: "form-control u_set_li"%>

        <%= f.submit"登録",	class: "btn btn-primary" %>
	<%end%>