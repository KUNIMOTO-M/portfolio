<div class="microposts">
  <div class="title_container"> 
    <h1 class="content">アップロード</h1>
  </div> 
    
	<div class="notice_container">
		<div class="row">
			<div class="col-md-6 col-md-offset-3">	
				<%= form_with model: @file do |f| %>
				<%= f.label :title %>
				<%= f.text_field :title, class: "form-control u_set_li"%>

				<%= f.label :file%>
				<%= f.file_field :file, class: "form-control u_set_li"%>
				<%= f.submit"登録",	class: "btn btn-primary" %>

				<%end%>
			</div>
		</div>
	</div>
</div>